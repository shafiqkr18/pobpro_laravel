/*Table structure for table `plans` */

DROP TABLE IF EXISTS `plans`;

CREATE TABLE `plans` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `subject` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `budget` decimal(18,2) DEFAULT NULL,
  `details` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `attachments` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `start_date` date DEFAULT NULL,
  `end_date` date DEFAULT NULL,
  `is_open` smallint(3) NOT NULL DEFAULT '1',
  `is_draft` smallint(1) NOT NULL DEFAULT '0',
  `is_approved` smallint(5) NOT NULL DEFAULT '0' COMMENT '0=pending,1=gm approved',
  `created_by` int(11) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  `deleted_at` timestamp NULL DEFAULT NULL,
  `company_id` int(11) NOT NULL DEFAULT '0',
  `plan_budget` int(11) NOT NULL DEFAULT '0',
  `recruitment_type_id` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=51 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

