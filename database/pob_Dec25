USE `pobpro`;

/*Table structure for table `activities` */

DROP TABLE IF EXISTS `activities`;

CREATE TABLE `activities` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `type` enum('offers','interviews','letters') CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `listing_id` int(11) DEFAULT '0',
  `action_type` smallint(5) DEFAULT '0' COMMENT '0=any,1=approved,2=rejected,3=created',
  `is_todo` tinyint(1) NOT NULL DEFAULT '0',
  `has_action` tinyint(1) NOT NULL DEFAULT '0',
  `activity_title` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `activity_details` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `company_id` int(11) DEFAULT '0',
  `created_by` int(11) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=59 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

