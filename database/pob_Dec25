  `id` int(11) NOT NULL AUTO_INCREMENT,
  `short_name` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `full_name` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

/*Data for the table `work_types` */

insert  into `work_types`(`id`,`short_name`,`full_name`) values 
(1,'FT','Full Time'),
(2,'PT','Part Time'),
(3,'CO','Contract'),
(4,'TP','Temporary'),
(5,'OT','Other');

