/*Table structure for table `position_vacancies` */

DROP TABLE IF EXISTS `position_vacancies`;

CREATE TABLE `position_vacancies` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `position_id` int(11) DEFAULT NULL,
  `job_ref_no` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `job_title` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `nationality` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `location` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `gender` varchar(55) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `age` int(11) DEFAULT NULL,
  `education_level` int(11) DEFAULT NULL,
  `work_type` varchar(55) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `job_description` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `salary` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `status` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT 'open',
  `created_by` int(11) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  `deleted_at` timestamp NULL DEFAULT NULL,
  `company_id` int(11) NOT NULL DEFAULT '0',
  `report_to` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `is_approved` smallint(5) NOT NULL DEFAULT '0' COMMENT '1=approved',
  `dept_id` int(11) NOT NULL DEFAULT '0',
  `rotation_pattern` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `job_purpose` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `duty_responsibility` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `certifications` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `experience_details` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `skills` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `others` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `attachments` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

