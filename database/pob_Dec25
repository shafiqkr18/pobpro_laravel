/*Table structure for table `offers` */

DROP TABLE IF EXISTS `offers`;

CREATE TABLE `offers` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `reference_no` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `position_id` int(11) DEFAULT NULL,
  `candidate_id` int(11) DEFAULT NULL,
  `type` smallint(5) DEFAULT '0' COMMENT '1= contract,0=offer',
  `position_type` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `report_to` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `work_start_date` date DEFAULT NULL,
  `subject` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `notes` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `attachments` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `accepted` smallint(3) NOT NULL DEFAULT '0',
  `dm_approved` smallint(3) NOT NULL DEFAULT '0',
  `hrm_approved` smallint(3) NOT NULL DEFAULT '0',
  `gm_approved` smallint(3) NOT NULL DEFAULT '0',
  `offer_amount` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `pay_type` smallint(5) NOT NULL DEFAULT '1' COMMENT '1=monthly,2=daily',
  `sending_status` smallint(3) NOT NULL DEFAULT '0' COMMENT '0=prepare,1=sent',
  `created_by` int(11) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  `deleted_at` timestamp NULL DEFAULT NULL,
  `location` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `plan_id` int(11) NOT NULL DEFAULT '0',
  `company_id` int(11) NOT NULL DEFAULT '0',
  `rotation_type_id` int(11) NOT NULL DEFAULT '0',
  `contract_duration_id` int(11) NOT NULL DEFAULT '0',
  `comments` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `hire_type` smallint(5) NOT NULL DEFAULT '1' COMMENT '1=local,2=expat',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=86 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

