/*Table structure for table `candidates` */

DROP TABLE IF EXISTS `candidates`;

CREATE TABLE `candidates` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `reference_no` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `badge_id` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `name` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `last_name` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `age` int(11) DEFAULT NULL,
  `gender` varchar(55) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `phone` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `skype` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `other_contact` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `email` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `position_id` int(11) DEFAULT '0',
  `old_position_id` int(11) NOT NULL DEFAULT '0',
  `plan_id` int(11) DEFAULT '0',
  `nationality` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `location` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `date_of_birth` date DEFAULT NULL,
  `major` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `introduction` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `work_experience` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `certifications` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `resume` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `other_benefits` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `expected_salary` int(11) DEFAULT '6',
  `fixed_salary` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `work_type` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT 'FT',
  `education_level` int(11) DEFAULT NULL,
  `notes` text CHARACTER SET latin1 COLLATE latin1_swedish_ci,
  `is_qualified` smallint(6) DEFAULT '0',
  `is_shortlisted` smallint(6) DEFAULT '0',
  `is_interviewed` smallint(6) DEFAULT '0',
  `is_enrolled` smallint(6) DEFAULT '0',
  `status` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT 'open',
  `created_by` int(11) DEFAULT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  `deleted_at` timestamp NULL DEFAULT NULL,
  `user_uuid` varchar(255) CHARACTER SET latin1 COLLATE latin1_swedish_ci DEFAULT NULL,
  `is_offered` int(11) NOT NULL DEFAULT '0',
  `is_contract` int(11) NOT NULL DEFAULT '0',
  `offer_accepted` int(11) NOT NULL DEFAULT '0',
  `contract_accepted` int(11) NOT NULL DEFAULT '0',
  `is_online` smallint(3) NOT NULL DEFAULT '0',
  `level` int(11) NOT NULL DEFAULT '5' COMMENT '1 to 10',
  `company_id` int(11) NOT NULL DEFAULT '0',
  `div_id` int(11) NOT NULL DEFAULT '0',
  `dept_id` int(11) NOT NULL DEFAULT '0',
  `section_id` int(11) NOT NULL DEFAULT '0',
  `ex_dept_id` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=223 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC;

/*Data for the table `candidates` */

